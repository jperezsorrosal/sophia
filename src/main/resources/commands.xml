<commands>
    <!-- split fastq into multiple bits -->
    <shell id="one">
        mkfifo /tmp/tmp1.fifo; cat dependency-cmds-extract.txt > <so>/tmp/tmp1.fifo</so>
    </shell>
    <shell id="two">
        cat <si>/tmp/tmp1.fifo</si> > <o>/tmp/tmp2.xml</o>
    </shell>

    <shell id="three">
        diff <i>/tmp/tmp2.xml</i> dependency-cmds-extract.txt
    </shell>

</commands>
